{
  "name": "ecommerce-express",
  "version": "0.0.0",
  "description": "A generic Ecommerce built over Express.js",
  "type": "module",
  "scripts": {
    "docker": "bash ./docker/run.sh",
    "docker:exec": "bash ./docker/exec.sh",
    "docker:into": "bash ./docker/into.sh",
    "docker:start": "npm run docker up",
    "build": "tsc && tsc-alias",
    "start": "concurrently -k \"npm run docker:start\" \"nodemon dist/src/app.js\"",
    "play": "concurrently -k \"npm run docker:start\" \"nodemon dist/src/playground.js\"",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rm -rf dist",
    "drizzle": "npx drizzle-kit",
    "seed:db": "npm run build && node dist/src/seeds.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "express",
    "nodejs",
    "rest-api",
    "web-application"
  ],
  "author": "Max Hernandez",
  "license": "MIT",
  "devDependencies": {
    "@sinclair/typebox": "^0.34.33",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/dotenv-defaults": "^2.0.4",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.17",
    "@types/morgan": "^1.9.9",
    "@types/pg": "^8.11.11",
    "@types/sinon": "^17.0.4",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "compression": "^1.8.0",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "dotenv-defaults": "^5.0.2",
    "drizzle-kit": "^0.30.6",
    "drizzle-typebox": "^0.3.2",
    "ejs": "^3.1.10",
    "eslint": "^9.23.0",
    "express": "^5.1.0",
    "file-type": "^21.0.0",
    "helmet": "^8.1.0",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.9",
    "openapi-types": "^12.1.3",
    "sinon": "^20.0.0",
    "supertest": "^7.1.1",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.3.4",
    "tsc-alias": "^1.8.13",
    "typescript": "^5.8.2",
    "url-join": "^5.0.0"
  },
  "dependencies": {
    "@types/node": "^22.13.15",
    "base64url": "^3.0.1",
    "drizzle-orm": "^0.41.0",
    "express-openapi-validator": "^5.5.2",
    "pg": "^8.14.1",
    "remove": "^0.1.5",
    "sharp": "^0.34.2",
    "swagger-jsdoc": "^6.2.8"
  }
}
